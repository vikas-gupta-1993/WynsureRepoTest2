TauxAnnuel : Num8
CapitalVerse : Num8
CapitalAcquis : Num8


procedure WFCalculate(inOut Result : Num8)
   var a : Num8
   var b : Num8
   
   a = self.Ln(self.CapitalAcquis / self.CapitalVerse)
   b = self.Ln(1 + self.TauxAnnuel)
   Result = a / b
endProc 

