<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>
<gui display="Var Diff">
  <intro height="30">
    <p align='center'>Var Diff - version 1.0</p>
  </intro>
  <command display="Var Diff">..\..\..\Free\xmldiff\xmldiff execute VarDiff.xds {xmlInput:quoted} {htmlOutput:quoted}</command>
  <arg id="xmlInput" type="file" display="Bundle Diff [In]" />
  <arg id="htmlOutput" type="file" mask="All Files (*.*)|*.*" display="VarDiff [out]">{'{xmlInput:dir}','VarDiff.html':join}</arg>
  <out id="out_varDiff" display="Diff" open-with="explorer.exe">{htmlOutput}</out>
</gui>