#
# $1 : AppDesc XML before [in]
# $2 : AppDesc XML after [in]
# $3 : AppDesc Diff XML [out]
options --auto-save no
print "AppDesc Diff"
print "Pre-traitement du fichier 1"
xslt appdesc_fix_id.xsl $1 before
print "Pre-traitement du fichier 2"
xslt appdesc_fix_id.xsl $2 after
diff before after output
discard before
discard after
save $3 output

print "Done."