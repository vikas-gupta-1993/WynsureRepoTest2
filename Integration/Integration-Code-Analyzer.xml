<?xml version="1.0" encoding="ISO-8859-1" ?>
<aWEXIntegrationPlanDesc name="Integration-Code Analyzer">
	<aWEXIntegCodeAnalyzerDesc name="Code Analyzer">
		<theCodeAnalyzer>
			<aCodeAnalyzer name="Code analyzer MM errors" create="True">
				<EntitiesToAnalyze>
					<aWAMContainer name="All WF5800-3"/>
				</EntitiesToAnalyze>
				<Rules>
					<aWEXPerformanceOQLCAR name="OQL Performance Rule" create="True">
						<MustAnalyze>True</MustAnalyze>
						<MustApplyRule_MissingCursor>Enable</MustApplyRule_MissingCursor>
						<MustApplyRule_MissingPlusPlus>Skip tagged code</MustApplyRule_MissingPlusPlus>
						<MustApplyRule_AnalyzeJoins>Enable</MustApplyRule_AnalyzeJoins>
						<MustApplyRule_LeftOuterJoin>Disable</MustApplyRule_LeftOuterJoin>
						<MustApplyRule_JoinInOr>Enable</MustApplyRule_JoinInOr>
						<MustApplyRule_HasDistinct>Disable</MustApplyRule_HasDistinct>
						<MustApplyRule_DuplicatesInSelect>Disable</MustApplyRule_DuplicatesInSelect>
						<MustApplyRule_TopCursor>Disable</MustApplyRule_TopCursor>
					</aWEXPerformanceOQLCAR>	
				</Rules>
			</aCodeAnalyzer>
		</theCodeAnalyzer>
	</aWEXIntegCodeAnalyzerDesc>
	<aWEXIntegCodeAnalyzerDesc name="Code Analyzer">
		<theCodeAnalyzer>
			<aCodeAnalyzer name="Code analyzer MM errors" create="True">
				<EntitiesToAnalyze>
					<aWAMContainer name="All WF5800-3"/>
				</EntitiesToAnalyze>
				<Rules>
					<aWFxSearchForMMErrors name="MM errors" create="True">
						<MustAnalyze>True</MustAnalyze>
					</aWFxSearchForMMErrors>
				</Rules>
			</aCodeAnalyzer>
		</theCodeAnalyzer>
	</aWEXIntegCodeAnalyzerDesc>
	<aWEXIntegCodeAnalyzerDesc name="Code Analyzer">
		<theCodeAnalyzer>
			<aCodeAnalyzer name="Code analyzer Inconsistent Overrides" create="True">
				<EntitiesToAnalyze>
					<aWAMContainer name="All WF5800-3"/>
				</EntitiesToAnalyze>
				<Rules>
					<aWFxInconsistentReftosCAR name="Find Inconsistent Overrides" create="True">
						<MustAnalyze>True</MustAnalyze>
						<WarnForReferenceTypesOnly>False</WarnForReferenceTypesOnly>
						<IgnorePointerConversions>False</IgnorePointerConversions>
					</aWFxInconsistentReftosCAR>
				</Rules>
			</aCodeAnalyzer>
		</theCodeAnalyzer>
	</aWEXIntegCodeAnalyzerDesc>
</aWEXIntegrationPlanDesc>

