	<CheckListCtrlLineFormats>True</CheckListCtrlLineFormats>
	<CheckScreenSize>True</CheckScreenSize>
	<ScreenSizeMaxWidth>1024</ScreenSizeMaxWidth>
	<ScreenSizeMaxHeight>740</ScreenSizeMaxHeight>
