<aWFxSearchForMMErrors name="MM errors" create="True">
	<MustAnalyze>True</MustAnalyze>
</aWFxSearchForMMErrors>
<aWFxUnusedLocalsCAR name="Unused Locals" create="True">
	<MustAnalyze>True</MustAnalyze>
	<UnusedLocals>True</UnusedLocals>
	<UnusedConsts>True</UnusedConsts>
	<UnusedTypes>False</UnusedTypes>
	<SkipInterfaceClasses>True</SkipInterfaceClasses>
</aWFxUnusedLocalsCAR>
<aWFxSearchForMultiCurrencyCAR name="Multi-Currency" create="True">
	<MustAnalyze>True</MustAnalyze>
